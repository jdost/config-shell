export MPD_HOST=localhost
export MPD_PORT=9009

[[ $PATH != *$HOME/.bin* ]] && export PATH=~/.bin:$PATH
[[ $PATH != *.cabal* ]] && export PATH=$PATH:~/.cabal/bin
[[ $PATH != *heroku* ]] && export PATH=$PATH:/usr/local/heroku/bin
[[ $NODE_PATH != *jsctags* ]] && export NODE_PATH=$NODE_PATH:/usr/lib/jsctags

